{
  "name": "@chris-zhu/utils",
  "version": "0.0.2",
  "description": "Some common utils fns just you need",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "@chris-zhu",
  "license": "MIT",
  "scripts": {
    "start": "esno src/index.ts",
    "dev": "nr build -- --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "eslint \"{src,test,scripts}/**/{*.ts,*.mjs}\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "zx scripts/release.mjs",
    "removeTag": "zx scripts/removeTag.mjs"
  },
  "keywords": [
    "utils"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chris-zhu/utils.git"
  },
  "bugs": {
    "url": "https://github.com/chris-zhu/utils/issues"
  },
  "homepage": "https://github.com/chris-zhu/utils#readme",
  "devDependencies": {
    "@antfu/ni": "^0.12.0",
    "@chris-zhu/eslint-config": "^0.0.3",
    "@types/node": "^17.0.5",
    "bumpp": "^7.1.1",
    "child_process": "^1.0.2",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.5.0",
    "esno": "^0.13.0",
    "fs-extra": "^10.0.0",
    "tsup": "^5.11.9",
    "typescript": "^4.5.4",
    "vite": "^2.7.9",
    "vitest": "^0.0.122",
    "zx": "^4.2.0"
  }
}
