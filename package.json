    "@types/node": "^17.0.43",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vitest": "^0.15.0"
  }
